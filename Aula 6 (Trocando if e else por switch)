String
  tela = 'Contato';
  
  switch (tela) {
    case 'Home':
      print ('Página Principal');
      break;
      
      case 'splashScreen':
      print ('Splash Screen');
      break;
      
    default:
        print('Tela');
        break;
  }
  
  
  
